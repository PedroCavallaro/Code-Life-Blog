// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id         String   @id @default(uuid())
  postNumber Int      @default(autoincrement())
  title      String
  subtitle   String
  date       DateTime
  author     String
  tags       String[]
  driveId    String
}

model Email {
  id    String @id @default(uuid())
  email String @unique
}
